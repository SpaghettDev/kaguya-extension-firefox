{
    "name": "kaguya-extension",
    "version": "1.1.45",
    "description": "An extension that allow you to use Kaguya",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/hoangvu12/kaguya-extension"
    },
    "scripts": {
        "build": "tsc --noEmit && vite build",
        "build:watch": "cross-env __DEV__=true vite build -w",
        "build:hmr": "rollup --config utils/reload/rollup.config.ts",
        "wss": "node utils/reload/initReloadServer.js",
        "dev": "npm run build:hmr && (run-p wss build:watch)",
        "pack": "rimraf kaguya-extension.zip && jszip-cli add dist/* -o kaguya-extension.zip",
        "web-ext-pack": "web-ext build -s ./dist --overwrite-dest",
        "build-and-web-ext-pack": "npm run build && npm run web-ext-pack"
    },
    "type": "module",
    "dependencies": {
        "cheerio": "^1.0.0-rc.12",
        "crypto-js": "^4.2.0",
        "event-target-shim": "^6.0.2",
        "huge-uploader": "^1.0.6",
        "jszip-cli": "^1.4.24",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "^6.14.1",
        "rimraf": "^6.0.1"
    },
    "devDependencies": {
        "@ffflorian/jszip-cli": "^3.4.1",
        "@rollup/plugin-typescript": "^8.5.0",
        "@types/crypto-js": "^4.2.2",
        "@types/dom-parser": "^0.1.1",
        "@types/firefox": "0.0.34",
        "@types/firefox-webext-browser": "^120.0.4",
        "@types/jest": "29.0.3",
        "@types/node": "18.15.11",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.2.4",
        "@types/ws": "^8.5.4",
        "@typescript-eslint/eslint-plugin": "5.56.0",
        "@typescript-eslint/parser": "5.38.1",
        "@vitejs/plugin-react": "2.2.0",
        "autoprefixer": "^10.4.14",
        "chokidar": "^3.5.3",
        "cross-env": "^7.0.3",
        "eslint": "8.36.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "fs-extra": "11.1.0",
        "jest": "29.0.3",
        "jest-environment-jsdom": "29.5.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.24",
        "prettier": "2.8.8",
        "rollup": "2.79.1",
        "sass": "1.62.1",
        "tailwindcss": "^3.3.2",
        "ts-jest": "29.0.2",
        "ts-loader": "9.4.2",
        "typescript": "4.8.3",
        "vite": "3.1.3",
        "ws": "8.13.0"
    }
}
